{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>

    </tr>
    </thead>
    <tbody>
{{#article}}
    <tr>
        <th>{{id}}</th>
        <td >{{title}}</td>
        <td>{{content}}</td>
    </tr>
{{/article}}
    </tbody>
</table>
<a href="/articles/{{article.id}}/edit" class="btn btn-primary">Edit</a> <!-- #으로 열고 /으로 닫는게 아니면 점을 써준다-->
<a href="/articles/{{article.id}}/delete" class="btn btn-danger">DELETE</a>
<a href="/articles">Go to Articles</a>
{{>layouts/footer}}